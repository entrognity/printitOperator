<h3>Order Details</h3>

<div class="orderDetailPopupcontainer">
    <!-- Order Information -->

    <% orderDetail.forEach(order=> { %>
    <div class="orderInfo1">
        <p><strong>Order ID:</strong> <%= order.orderID %></p>
        <p><strong>Order Placed on:</strong> <%= order.createdAt.toDateString() %></p>
        <p><strong>Note:</strong> <%= order.note %></p>
        <p><strong>Call Before Print:</strong> <%= order.callBeforePrint %></p>
        <p><strong>Customer Name:</strong> <%= order.orderId %></p>
        <p><strong>Customer Contact:</strong> <%= order.userID %></p>
        <p><strong>Order Status:</strong> <%= order.orderStatus %></p>
        <p><strong>Order Amount:</strong> <%= order.orderAmount %></p>
        <p><strong>Payment Received:</strong> <%= order.paymentReceived %></p>
        <p><strong>Delivery Option:</strong> <%= order.deliveryOption %></p>
    </div>

    <!-- Customer Information -->
    <div class="orderInfo2">
        <p><strong>Service ID:</strong> <%= order.serviceID %></p>
        <p><strong>Service Name:</strong> <%= order.pages %></p>
        <p><strong>Total Pages:</strong> <%= order.pages %></p>
        <p><strong>Files:</strong> <%= order.filesUri %></p>
    </div>
    <% }) %>
</div>

